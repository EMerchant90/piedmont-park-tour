{
  "name": "piedmont-park-tour",
  "version": "0.1.0",
  "description": "trying out higher order components, passing props around, child to parent/parent to child",
  "main": "index.js",
  "scripts": {  
    "start": "cd server && node index.js",
    "heroku-postbuild": "cd client && npm install --only=dev && npm run build",
    "dev": "cd server && nodemon index.js",
    "install": "run-s install:server",
    "install:server": "cd server && npm install"
  },
  "cacheDirectories": [
    "node_modules",
    "server/node_modules",
    "client/node_modules"
  ],
  "author": "PROB8",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^3.1.4",
    "express": "^4.14.1",
    "http-proxy-middleware": "^0.17.3",
    "npm-run-all": "^4.0.1",
    "xp": "^1.0.1"
  }
}
